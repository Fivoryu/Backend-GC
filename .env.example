DEBUG=true
SECRET_KEY=your-secret-key-here
FRONTEND_URL=https://your-frontend-url.vercel.app
FLUTTER_WEB_URL=https://your-flutter-web-url.com

# Database Configuration
DB_NAME=your_database_name
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_HOST=your_db_host
DB_PORT=5432

# Email Configuration
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USE_TLS=true
EMAIL_HOST_USER=your-email@gmail.com
EMAIL_HOST_PASSWORD=your-app-password

# AWS Configuration (for production)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_STORAGE_BUCKET_NAME=your-s3-bucket-name
AWS_S3_REGION_NAME=us-east-1

# AWS RDS Configuration (auto-configured by Elastic Beanstalk)
RDS_HOSTNAME=your-rds-hostname
RDS_DB_NAME=your-rds-db-name
RDS_USERNAME=your-rds-username
RDS_PASSWORD=your-rds-password
RDS_PORT=5432

# Production Settings
ALLOWED_HOSTS=your-domain.elasticbeanstalk.com,your-custom-domain.com
CORS_ALLOWED_ORIGINS=https://your-frontend-domain.com,https://your-other-domain.com

# Django Settings Module (for production)
DJANGO_SETTINGS_MODULE=config.aws_settings